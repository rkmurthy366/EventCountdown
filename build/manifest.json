{
  "manifest_version": 3,
  "name": "Event Countdown",
  "description": "A new tab extension to add events and display countdowns with customization options.",
  "version": "1.0",
  "action": {
    "default_popup": "index.html"
  },
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "background": {
    "service_worker": "background.js"
  },
  "permissions": ["storage"],
  "icons": {
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
